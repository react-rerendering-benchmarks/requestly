{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react",
    "allowJs": true,
    "checkJs": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "rootDir": "./src",
    "baseUrl": ".",
    "paths": {
      "@requestly/analytics-vendors": ["../../common/analytics-vendors/dist/analytics-vendors.js"]
    }
  },
  "exclude": ["node_modules", "dist"],
  "include": ["./src/**/*", "../../common/analytics-vendors/dist/**/*"]
}
